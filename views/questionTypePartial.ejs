    <label class="radio-inline" style="margin-right:28px;">
        <input type="radio"  ng-model="$parent.checkbox" ng-checked="checkbox =='2'"  ng-init="checkbox='<%=questionType%>'"  value="2"  ng-change="radioSelect('textFeild')"  
